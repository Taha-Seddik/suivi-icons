{
  "name": "suivi-icons",
  "version": "1.3.12",
  "description": "list of suivi icons",
  "author": "ProNoob702",
  "license": "MIT",
  "repository": "ProNoob702/suivi-icons",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "svgr": "svgr -d src/icons svg-icons/ --typescript --icon --replace-attr-values \"#323232=currentColor\"",
    "suivi-cli": "suivi-cli",
    "git-prep": "git add * && git commit -m 'zz' && git push --all",
    "lib-prep": "npm run svgr && npm run format && npm run suivi-cli && npm run build",
    "publish-prep": "npm version patch && npm publish && npm run deploy",
    "zouta": "npm run lib-prep && npm run git-prep && npm run publish-prep",
    "format": "prettier --write \"src/**/*.{tsx,ts}\""
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.5",
    "suivi-cli": "^1.0.3"
  },
  "files": [
    "dist"
  ]
}
